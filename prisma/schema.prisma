generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Veiculo {
  id        Int      @id @default(autoincrement())
  veiculo   String
  marca     String
  ano       String
  descricao  String   @db.Text
  vendido   Boolean
  created   DateTime
  updated   DateTime

  @@map("veiculos")
}
